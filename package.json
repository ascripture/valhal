{
  "version": "0.8.9",
  "license": "MIT",
  "main": "dist/valhal.js",
  "module": "dist/valhal.mjs",
  "typings": "dist/valhal.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "rollup -c",
    "test": "jest",
    "lint": "eslint --fix 'src/**/*.ts'",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "rxjs": "~6.6.0"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@ascripture/valhal",
  "author": "Philipp Schladitz",
  "size-limit": [
    {
      "path": "dist/valhal.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/valhal.mjs",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^5.0.3",
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "esbuild": "^0.13.3",
    "eslint": "^7.32.0",
    "husky": "^7.0.2",
    "jest": "^27.2.4",
    "rollup": "^2.58.0",
    "rollup-plugin-dts": "^4.0.0",
    "rollup-plugin-esbuild": "^4.5.0",
    "rxjs": "~6.6.0",
    "size-limit": "^5.0.3",
    "ts-jest": "^27.0.5",
    "tslib": "^2.3.1",
    "typescript": "^4.4.2"
  },
  "description": "A store library with support for caching and entities.",
  "directories": {
    "test": "test"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/*.spec.ts"
    ]
  }
}
